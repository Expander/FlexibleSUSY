@ModelName@_low_scale_constraint<Two_scale>::@ModelName@_low_scale_constraint(
   @ModelName@<Two_scale>* model_, const softsusy::QedQcd& qedqcd_)
   : model(model_)
   , qedqcd(qedqcd_)
{
   initialize();
}

void @ModelName@_low_scale_constraint<Two_scale>::apply()
{
   check_model_ptr();

@initialSetting@

   model->calculate_DRbar_masses();
   update_scale();
   qedqcd.run_to(scale, 1.0e-5);
   calculate_DRbar_gauge_couplings();
   calculate_running_SM_masses();

@applyConstraint@
}

const Eigen::Matrix<std::complex<double>,3,3>& @ModelName@_low_scale_constraint<Two_scale>::get_ckm()
{
