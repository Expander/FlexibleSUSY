const int @ModelName@_observables::NUMBER_OF_OBSERVABLES;

@ModelName@_observables::@ModelName@_observables()
@observablesInit@
{
}

Eigen::ArrayXd @ModelName@_observables::get() const
{
@getObservables@
   return vec;
}

std::vector<std::string> @ModelName@_observables::get_names()
{
@getObservablesNames@
   return names;
}

void @ModelName@_observables::clear()
{
@clearObservables@
}

void @ModelName@_observables::set(const Eigen::ArrayXd& vec)
{
@setObservables@
}

@ModelName@_observables calculate_observables(@ModelName@_mass_eigenstates& model,
                                              const softsusy::QedQcd& qedqcd,
                                              const Physical_input& physical_input,
                                              double scale)
{
   auto model_at_scale = model;

   if (scale > 0.) {
      try {
         model_at_scale.run_to(scale);
      } catch (const Error& e) {
         model.get_problems().flag_thrown(e.what_detailed());
         return @ModelName@_observables();
      } catch (const std::exception& e) {
         model.get_problems().flag_thrown(e.what());
         return @ModelName@_observables();
      }
   }

   return calculate_observables(model_at_scale, qedqcd, physical_input);
}

@ModelName@_observables calculate_observables(@ModelName@_mass_eigenstates& model,
                                              const softsusy::QedQcd& qedqcd,
                                              const Physical_input& physical_input)
{
   @ModelName@_observables observables;

   try {
@calculateObservables@
   } catch (const Error& e) {
      model.get_problems().flag_thrown(e.what_detailed());
   } catch (const std::exception& e) {
      model.get_problems().flag_thrown(e.what());
   }

   return observables;
}

} // namespace flexiblesusy
