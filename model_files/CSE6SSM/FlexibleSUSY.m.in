EWSBInitialGuess = {
   {m0Sq, m12^2},
   {TanTheta, 1},
   {vphi, sInput / 2},
   {\[Xi]F, sInput},
   {LXiF0, sInput}
};

EWSBSubstitutions = {
   {vs, sInput Cos[ArcTan[TanTheta]]},
   {vsb, sInput Sin[ArcTan[TanTheta]]}
};

SUSYScale = Sqrt[Product[M[Su[i]]^(Abs[ZU[i,3]]^2 + Abs[ZU[i,6]]^2), {i,6}]];

SUSYScaleFirstGuess = Sqrt[Azero^2 + 4 m12^2];

SUSYScaleInput = {
   {vs, sInput Cos[ArcTan[TanTheta]]},
   {vsb, sInput Sin[ArcTan[TanTheta]]}
};

HighScale = g1 == g2;

