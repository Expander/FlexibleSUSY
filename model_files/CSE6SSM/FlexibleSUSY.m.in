   {2, m12},
   {3, TanBeta},
   {5, Azero}
};

EXTPAR = {
   {65, sInput}
};

FSAuxiliaryParameterInfo = {
   {m0Sq,     { ParameterDimensions -> {1},
                MassDimension -> 2 } },
   {m12,      { MassDimension -> 1 } },
   {Azero,    { MassDimension -> 1 } },
   {TanBeta,  { MassDimension -> 0 } },
   {TanTheta, { ParameterDimensions -> {1},
